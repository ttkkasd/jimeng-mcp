# Example configuration for environment variables
# Copy this file to .env and fill in your actual credentials

# Volcengine API credentials
VOLCENGINE_ACCESS_KEY_ID=your_access_key_id_here
VOLCENGINE_SECRET_ACCESS_KEY=your_secret_access_key_here

# Optional: Set different region if needed (default: cn-north-1)
# VOLCENGINE_REGION=cn-north-1

# Optional: Set different service if needed (default: cv)
# VOLCENGINE_SERVICE=cv